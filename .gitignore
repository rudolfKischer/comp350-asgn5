# MATLAB .gitignore

# Ignore all .mat files
*.mat

# Ignore Simulink files
*.mdl
*.slx
*.slxp

# Ignore autogenerated files by MATLAB
*.mex*
*.p
mex_*.dll
mex_*.mexw64
mex_*.mexa64
mex_*.mexmaci64

# Ignore autosave files
*.asv
*.autosave

# Ignore S-Function builder files
sfprj/build/

# Ignore MATLAB code analyzer reports
*.mlint

# Ignore MATLAB preferences directory
matlabprefs.mat
matlab.settings

# Ignore Simulink cache and code generation files
slprj/
sl_cache/
slcov/

# Ignore MATLAB toolstrip customizations
*.ctf

# Ignore files related to MATLAB packaging
*.prj
*.mlappinstall
*.mlpkginstall

# Ignore MATLAB R2022a and newer Simulink Projects
*.slxproj

# Ignore MATLAB GUI layout
*.mlapp

# Ignore version control files
*.git
*.svn

# Ignore temporary files created by MATLAB
*.mexmaci64~
*.mexw64~
*.mexa64~
*.tlc
*.tmw
*.log
*.dat
*.sdi

# Ignore local configuration files
local_settings.m

# Ignore directories for MATLAB unit tests
tests/

# Ignore MATLAB dependency analysis
*.depfun

# Ignore MATLAB live scripts
*.mlx

# Ignore MATLAB crash dump files
matlab_crash_dump.*

# Ignore files associated with MATLAB's web-based documentation
helpsearch/

# Ignore Simulink Project Autosave files
*.autosave

resources/

html/

.DS_Store